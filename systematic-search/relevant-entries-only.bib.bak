  keywords  = {data structures;information retrieval;social networking (online);data model;hypergraph data structure;multimedia content;multimedia information retrieval activities;multimedia ranking functions;multimedia social networks;social communities;social media networks;user-content interaction modeling;Data models;Flickr;Multimedia communication;Streaming media;Twitter;Multimedia;Ranking;Social Network Analysis},
}

@InProceedings{7155020,
