@InProceedings{7549325,
  author    = {S. On-at and A. Quirin and A. Péninou and N. Baptiste-Jessel and M. F. Canut and F. Sèdes},
  title     = {Taking into account the evolution of users social profile: Experiments on Twitter and some learned lessons},
  booktitle = {2016 IEEE Tenth International Conference on Research Challenges in Information Science (RCIS)},
  year      = {2016},
  pages     = {1-12},
  month     = {June},
  abstract  = {Incorporating user interests evolution over time is a crucial problem in user profiling. We particularly focus on social profiling process that uses information shared on user social network to extract his/her interests. In this work, we apply our existing time-aware social profiling method on Twitter. The aim of this study is to measure the effectiveness of our approach on this kind of social network platform, which has different characteristics from those of other social networking sites. Although the improvement compared to the time-agnostic baseline method is still low, the experiments using a parametric study showed us the benefit of applying a time-aware social profiling process on Twitter. We also found that our method performs well on sparse networks and that the information dynamic influences more the quality of our proposed time-aware method than the relationships dynamic while building the social profile on Twitter. This observation will lead us to a more complex study to find out meaningful factors to incorporate user interests evolution on social profiling process in such a network.},
  doi       = {10.1109/RCIS.2016.7549325},
  keywords  = {social networking (online);Twitter;dynamic relationships;social networking sites;sparse networks;time-aware user social profiling method;user interest evolution;user social network;Buildings;Context;Data mining;Feature extraction;Measurement;Twitter;Egocentric network;Social profile;Time-aware method;User profile},
}

@InProceedings{7844925,
  author    = {D. T. Hoang and D. Hwang and V. C. Tran and V. D. Nguyen and N. T. Nguyen},
  title     = {Academic event recommendation based on research similarity and exploring interaction between authors},
  booktitle = {2016 IEEE International Conference on Systems, Man, and Cybernetics (SMC)},
  year      = {2016},
  pages     = {004411-004416},
  month     = {Oct},
  abstract  = {In this study, a new academic event recommendation method is proposed. This method analyzes author interactions, academic event attendance records, research related, and textual descriptions from attended academic events to measure interaction strength between authors. Experiments on the DBLP dataset and Wiki Calls for Papers (WikiCFP) showed that the proposed method is helpful in improving the accuracy of a recommendation system in comparison with other methods. In addition, this method can be applied to various recommended tasks such as collaboration recommendation, papers recommendation, etc.},
  doi       = {10.1109/SMC.2016.7844925},
  keywords  = {Big Data;Web sites;educational administrative data processing;recommender systems;DBLP dataset;Wiki Calls for Papers;WikiCFP;academic event attendance records;academic event recommendation;author interaction analysis;collaboration recommendation;paper recommendation;research similarity;textual descriptions;Collaboration;Computational modeling;Conferences;Cybernetics;Internet;XML},
