  keywords  = {information retrieval;knowledge acquisition;ontologies (artificial intelligence);crowd-sourcing methods;human knowledge;knowledge acquisition bottleneck;ontology extraction;resource navigation;resource retrieval;search precision;Aggregates;Computational intelligence;Data mining;Grid computing;Humans;Information retrieval;Knowledge acquisition;Ontologies;Terminology;Vocabulary},
}

@InProceedings{5364933,
  author    = {S. Koshman and C. J. Lu},
  title     = {Comparing visualization techniques to structure collaborative concepts},
  booktitle = {2009 5th International Conference on Collaborative Computing: Networking, Applications and Worksharing},
  year      = {2009},
  pages     = {1-8},
  month     = {Nov},
  abstract  = {Much research has been devoted to collaborative tools for writing, group communication, reviewing, managing projects, and developing network infrastructures to support sharing activities. However the creation and display of foundational collaborative concepts that manage group work has not been paid significant attention. Information visualization potentially addresses the progression of distributed concept building and presentation by visually representing group-generated concepts. This paper analyzes current visualization-based techniques including tag clouds, node-link diagrams, and mapping structures in a web-based environment. The analysis contributes to exploring the use of visualization tools for creating group concept structures that organize ideas for documents, projects, or specifications in academia and industry. A comparative features table is built and used as a basis for presenting an initial interface design to develop a collaborative visualization tool for concept management. Implications for future work include visualization system interface design, user evaluation, and adaptation to mobile collaborative visualization environments.},
  doi       = {10.4108/ICST.COLLABORATECOM2009.8351},
  keywords  = {Internet;data visualisation;graphical user interfaces;groupware;mobile computing;Web-based environment;collaborative tools;information visualization;interface design;mobile collaborative visualization environments;structure collaborative concepts;Buildings;Collaboration;Collaborative tools;Collaborative work;Data visualization;Displays;Humans;Project management;Tag clouds;Writing;collaborative work;design;user interfaces;visualization},
}

