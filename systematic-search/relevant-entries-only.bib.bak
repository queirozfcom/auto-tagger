@InProceedings{7395824,
  author    = {Y. G. Cinar and S. Zoghbi and M. F. Moens},
  title     = {Inferring User Interests on Social Media from Text and Images},
  booktitle = {2015 IEEE International Conference on Data Mining Workshop (ICDMW)},
  year      = {2015},
  pages     = {1342-1347},
  abstract  = {Inferring user interests on social media from text and images is addressed as a multi-class classification problem. We proposed approaches to infer user interest on Social media where often multi-modal data (text, image etc.) exists. We use user-generated data from Pinterest.com as a natural expression of users' interests. We consider each pin (image-text pair) as a category label that represents a broad user interest, since users collect images that they like on the social media platform and often assign a category label. This task is useful beyond Pinterest because most user-generated data on the Web is not necessarily readily categorized into interest labels. In addition to predicting users' interests, our main contribution is exploiting a multi-modal space composed of images and text. This is a natural approach since humans express their interests with a combination of modalities. Exploiting multi-modal spaces in this context has received little attention in the literature. We performed eleven experiments using the state-of-the-art image and textual representations, such as convolutional neural networks, word embeddings, and bags of visual and textual words. Our experimental results show that in fact jointly processing image and text increases the overall interest classification accuracy, when compared to uni-modal representations (i.e., using only text or using only images).},
  keywords  = {human factors;image representation;neural nets;pattern classification;social networking (online);text analysis;Pinterest.com;World Wide Web;bags of visual words;convolutional neural networks;image processing;image representation;multiclass classification problem;multimodal data;multimodal space;social media platform;textual representation;textual words;user interests;user-generated data;word embeddings;Histograms;Image representation;Media;Neural networks;Pins;Semantics;Visualization;bag of words (BoW);convolutional neural networks (CNN);inferring user interests;term frequencies;user modeling;word embeddings},
}

@InProceedings{4573146,
  author    = {S. S. Lee and H. S. Yong},
  title     = {OntoSonomy: Ontology-Based Extension of Folksonomy},
  booktitle = {2008 IEEE International Workshop on Semantic Computing and Applications},
  year      = {2008},
  pages     = {27-32},
  month     = {July},
  abstract  = {While collaborative tagging is attractive because of their easy and simple way to tag and search, tag based search suffers from no semantic descriptions for contents. Semantic Web technologies provide a new way to annotating and retrieving images. We address the problems of both folksonomy and semantic Web annotation systems for image retrieval in particular and argue that this can be improved by giving meanings to users' tags. This paper considers using both minimized domain-specific ontology and generic ontology. We show how less detailed ontologies can help end users to annotate and search with the simple user interface. Our prototype system has the capability to describe the semantics for contents of an image. Also, no previous extra work to create instances according to the ontology for later annotations is needed.},
