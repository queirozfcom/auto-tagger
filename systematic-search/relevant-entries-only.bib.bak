  keywords  = {groupware;social networking (online);Email;IM;collaboration awareness;collaborative work;lightweight informal communication media;microblogging;project teams;social software;wikis;Collaboration;Companies;Electronic mail;Interviews;Media;Social network services;Software;Awareness in Collaboration Systems;Social Software Based Collaboration},
}

@InProceedings{6511968,
