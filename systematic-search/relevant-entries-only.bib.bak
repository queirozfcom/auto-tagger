}

@InProceedings{Dicheva2010,
  author    = {D. Dicheva and C. Dichev},
  title     = {Finding Resources and Collaborators within Digital Collections},
  booktitle = {2010 IEEE/WIC/ACM International Conference on Web Intelligence and Intelligent Agent Technology},
