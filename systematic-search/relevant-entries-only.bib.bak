  keywords  = {multimedia computing;music;recommender systems;user interfaces;Canoris API;audio analysis;content-based preference elicitation;content-based system;digital music;music recommendation;popular online music service;raw audio information;semantic descriptor mapping;semantic music similarity measure;semantic notion;user musical preference visualization;Avatars;Data visualization;Mood;Music;Recommender systems;Semantics;Visualization},
}

@InProceedings{6062165,
  author    = {S. Haiduc and J. Aponte and A. Marcus},
  title     = {Supporting program comprehension with source code summarization},
  booktitle = {2010 ACM/IEEE 32nd International Conference on Software Engineering},
  year      = {2010},
  volume    = {2},
