  keywords  = {Internet;information retrieval;knowledge based systems;knowledge representation languages;mathematics computing;natural language processing;ontologies (artificial intelligence);scientific information systems;text analysis;OWL;characteristic linguistic structures;graphical representation;information retrieval;intelligent retrieval mechanism;knowledge base;mathematical texts;natural language processing;scientific digital publications;Books;Data mining;Equations;Information retrieval;Knowledge management;Logic;Mathematical model;Natural language processing;OWL;Ontologies},
}

@InProceedings{5437610,
