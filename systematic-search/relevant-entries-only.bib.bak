@InProceedings{7817066,
  author    = {Z. Meng and F. Gandon and C. F. Zucker},
  title     = {Joint Model of Topics, Expertises, Activities and Trends for Question Answering Web Applications},
  booktitle = {2016 IEEE/WIC/ACM International Conference on Web Intelligence (WI)},
  year      = {2016},
  pages     = {296-303},
