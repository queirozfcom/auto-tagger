@InProceedings{6340210,
  author    = {S. Hamdi and A. Lopes Gancarski and A. Bouzeghoub and S. Ben Yahia},
  title     = {Enriching the DBpedia Ontology with Shared Conceptualizations from Folksonomies},
  booktitle = {2012 IEEE 36th Annual Computer Software and Applications Conference},
  year      = {2012},
  pages     = {551-556},
  month     = {July},
  abstract  = {Folksonomy systems and social bookmarking tools are rapidly spreading on the Web. Their steady increase reveals a more dynamic and interactive space, in which users can individually and freely share online resources and assign terms to them. In this paper, we address the problem of how to exploit these rich systems to enrich existing ontologies. Our approach taps into external tools such as the sense inventory WordNet to fix mistakes resulting from the free tagging and takes advantage from semantic relationships between tags to recognize resources' context. We carried out evaluations that mainly paid attention on the particularly challenging problem of resources' ambiguity to prove that not all resources shared in folksonomies are relevant resources for Dbpedia ontology. In this respect, it is worth of mention that our pre-processing step increases precision values to achieve an average precision of 80%.},
  doi       = {10.1109/COMPSAC.2012.74},
  issn      = {0730-3157},
  keywords  = {Internet;ontologies (artificial intelligence);social networking (online);DBpedia ontology;WordNet;folksonomies;free tagging;online resource sharing;resource context;sense inventory;social bookmarking tools;Context;Google;Java;Ontologies;Semantics;Software;Tagging;folksonomy; ontology; enrichment;tags context},
}

@InProceedings{7280071,
  author    = {Divjot and J. Singh},
  title     = {Effective Model and Implementation of Dynamic Ranking in Web Pages},
  booktitle = {2015 Fifth International Conference on Communication Systems and Network Technologies},
  year      = {2015},
  pages     = {1010-1014},
  month     = {April},
  abstract  = {Web page ranking algorithms are used to score the universal resource locators or simply online links of the Web applications. The corporate world strives to develop the Web applications in such a way so that it can be visible on the top results in the major search engines and search directories. A number of Web page ranking algorithms are developed with different scientific approaches making use of assorted mathematical formulations. Google Page Rank, Yahoo Web Rank, HITS Algorithm, Tag Rank Algorithm, Weighted Links Rank Algorithm, Relation Based Algorithm, Time Rank Algorithm and many other approaches are used in the industry so that the accurate and effective scoring can be done. In this research paper, the effective algorithmic approach based on the enhancement in HITS algorithm is performed. This work focuses on the implementation of augmentation in the classical HITS algorithm. The results depict the proposed approach providing effective and long term unbiased scoring of the Web applications. In this research work, the implementation of deep link validity is performed and the results are obtained in terms of deep scoring and execution time.},
  doi       = {10.1109/CSNT.2015.261},
  keywords  = {Web sites;search engines;Google Page Rank;HITS algorithm;Web applications;Web page ranking algorithms;Yahoo Web Rank;deep link validity;deep scoring;dynamic ranking;execution time;long term unbiased scoring;online links;relation based algorithm;search directories;search engines;tag rank algorithm;time rank algorithm;universal resource locators;weighted links rank algorithm;Algorithm design and analysis;Crawlers;Search engines;Semantic Web;Web mining;Web pages;HITS Algorithm;Web Mining;Web Page Ranking},
}

@InProceedings{7178054,
  author    = {M. Buccoli and A. Gallo and M. Zanoni and A. Sarti and S. Tubaro},
  title     = {A Dimensional Contextual Semantic Model for music description and retrieval},
  booktitle = {2015 IEEE International Conference on Acoustics, Speech and Signal Processing (ICASSP)},
  year      = {2015},
  pages     = {673-677},
  month     = {April},
  abstract  = {Several paradigms for high-level music descriptions have been proposed to develop effective system for browsing and retrieving musical content in large repositories. Such paradigms are based on either categorical or dimensional models. The interest in dimensional models has recently grown a great deal, as they define a semantic relation between concepts through graded descriptions. One problem that affects semantic descriptions is the ambiguity that often arises from using the same descriptor in different contexts. In order to overcome this difficulty, it is important to model and address polysemy, which is the property of words to take on different meanings depending on the use-context. In this paper we propose a Dimensional Contextual Semantic Model for defining semantic relations among descriptors in a context-aware fashion. This model is here used for developing a semantic music search engine. In order to evaluate the effectiveness of our model, we compare this engine with two systems that are based on different description models.},
  doi       = {10.1109/ICASSP.2015.7178054},
  issn      = {1520-6149},
  keywords  = {information retrieval;music;search engines;context-aware fashion;dimensional contextual semantic model;graded descriptions;high-level music descriptions;music retrieval;musical content;musical content retrieval;polysemy;semantic descriptions;semantic music search engine;use-context;Computational modeling;Context;Context modeling;Large scale integration;Mood;Search engines;Semantics;information retrieval;music;music recommendation;polysemy;semantic contexts},
}

