  keywords = {information retrieval;recommender systems;folksonomy-based recommendation;published resources;recommender systems;resource similarity;semantic similarity;social tagging sites;tagging systems;user profiles;web pages;Decision support systems;Handheld computers;Image processing;Internet;Q measurement;Semantics;Tagging;folksonomies;recommender systems;similarity measures;social tagging systems},
