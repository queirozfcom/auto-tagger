@InProceedings{7810836,
  author    = {H. X. Xiong and J. Hu and X. Yang},
  title     = {A Content-Based Intelligent Ranking Model for Microblog},
  booktitle = {2015 International Conference on Computer Science and Applications (CSA)},
  year      = {2015},
  pages     = {73-76},
  month     = {Nov},
  abstract  = {This paper firstly analyzes drawbacks of sequencing in microblog, and then proposed a ranking model based on contents to optimize the information flow of microblog. This model extracts characteristics which reflect users' interests from their microblog contents by text mining, and combines their tags to construct interest model for them, then calculates and ranks the weight of users' information flow in their homepage in terms of the user interest model to help users preferentially scan the microblog content which they interested in mostly. After verified via R programming and a real example, this model is effective.},
