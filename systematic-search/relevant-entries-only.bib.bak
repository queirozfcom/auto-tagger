@InCollection{Smiraglia201541,
  author    = {Richard P. Smiraglia},
  title     = {3 - Empirical methods for visualizing domains},
  booktitle = {Domain Analysis for Knowledge Organization},
  publisher = {Chandos Publishing},
  year      = {2015},
  editor    = {Smiraglia, Richard P.},
  pages     = {41 - 50},
  isbn      = {978-0-08-100150-9},
  abstract  = {Abstract At the simplest level, domain analysis simply means capturing the knowledge base of a community. This chapter is devoted to explication of the typical or standard approaches to capturing the knowledge base of a community. How we go about capturing this knowledge base, specifically, is what constitutes our domain analytical methodology. A methodology is a plan for research that quite specifically lays out which tools will be used and why. Operational parameters for domain analytic research require that the context is in the first instance a unit of analysis for the construction of a knowledge organization system. Whether it is our intent immediately to construct such a system or to leave it for others in subsequent work, our purpose in unraveling the extension and intension of a domain is to ascertain the concepts and their characteristics that constitute the functional knowledge base of the community under consideration. The second consideration is that a domain is a group. Methodologically then, the unit of analysis must be, in some way, a visible functional group. The remaining aspects of a domain that can be extracted by analysis are an ontological base that reveals an underlying teleology; a set of common hypotheses; an epistemological consensus on methodological approaches; and, social semantics. Any or all of the empirical methods described in Chapter 4, such as term extraction, thematic analysis, and visualization of author cocitation clusters should point to these aspects of domain coherence. },
  doi       = {https://doi.org/10.1016/B978-0-08-100150-9.00003-1},
  keywords  = {Knowledge base, Community, Methodology, Hypotheses, Social semantics},
  url       = {http://www.sciencedirect.com/science/article/pii/B9780081001509000031},
}

