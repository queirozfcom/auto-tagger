@InProceedings{4438863,
  author    = {M. A. Hearst and D. Rosner},
  title     = {Tag Clouds: Data Analysis Tool or Social Signaller?},
  booktitle = {Proceedings of the 41st Annual Hawaii International Conference on System Sciences (HICSS 2008)},
  year      = {2008},
  pages     = {160-160},
  month     = {Jan},
  abstract  = {We examine the recent information visualization phenomenon known as tag clouds, which are an interesting combination of data visualization, web design element, and social marker. Using qualitative methods, we find evidence that those who use tag clouds do so primarily because they are perceived as having an inherently social or personal component, in that they suggest what a person or a group of people is doing or is interested in, and to some degree how that changes over time; they are visually dynamic and thus suggest activity; they are a compact alternative to a long list; they signal that a site has tags; and they are perceived as being fun, popular, and/or hip. The primary reasons people object to tag clouds are their visual aesthetics, their questionable usability, their popularity among certain design circles, and what is perceived as a bias towards popular ideas and the downgrading of alternative views.},
  doi       = {10.1109/HICSS.2008.422},
  issn      = {1530-1605},
  keywords  = {Internet;data visualisation;Web design element;data visualization;social marker;tag clouds;Data analysis;Data visualization;Frequency;Hip;Linux;Navigation;Operating systems;Tag clouds;Usability;Web design},
}

@Article{6226349,
  author   = {I. Marenzi and S. Zerr},
  title    = {Multiliteracies and Active Learning in CLIL amp;amp;#x2014;The Development of LearnWeb2.0},
  journal  = {IEEE Transactions on Learning Technologies},
  year     = {2012},
  volume   = {5},
  number   = {4},
  pages    = {336-348},
  month    = {Oct},
  issn     = {1939-1382},
  abstract = {This paper discusses the development of LearnWeb2.0, a search and collaboration environment for supporting searching, organizing, and sharing distributed resources, and our pedagogical setup based on the multiliteracies approach. In LearnWeb2.0, collaborative and active learning is supported through project-focused search and aggregation, with discussion and comments directly linked to the resources. We are developing the LearnWeb2.0 platform through an iterative evaluation-driven design-based research approach - this paper describes the first iteration and part of the second one. In the first iteration, we developed LearnWeb2.0 and evaluated it in two Content and Language Integrated Learning (CLIL) courses We followed the multiliteracies approach, using authentic content from a variety of sources and contexts to provide important input for CLIL. We present evaluation design and results for both courses, and discuss how the differences in both scenarios influenced student performance and satisfaction. In the second iteration, we improved LearnWeb2.0 based on these experiences - we describe improvements as well as problems addressed. Finally, we sketch the evaluation planned for the second cycle, and close with a reflection of our experiences with the design-based research approach for developing a collaborative learning environment, and on multiliteracies as a suitable approach for CLIL.},
  doi      = {10.1109/TLT.2012.14},
  keywords = {Internet;computer aided instruction;groupware;iterative methods;CLIL course;LearnWeb2.0;active learning;collaborative learning environment;content and language integrated learning courses;distributed resources;first iteration;iterative evaluation-driven design-based research approach;multiliteracies;project-focused search;search environment;second iteration;Collaboration;Context awareness;Decision support systems;Electronic leanring;Iterative methods;Natural language processing;Collaboration;Content and language integrated learning;Context awareness;Decision support systems;Electronic leanring;Iterative methods;Natural language processing;collaborative search;design-based research;multiliteracies},
}

@InProceedings{6528738,
  author    = {C. Haruechaiyasak and J. Pailai},
  title     = {ARSA: An archive social annotation tool for content digitization},
  booktitle = {2012 6th International Conference on New Trends in Information Science, Service Science and Data Mining (ISSDM2012)},
  year      = {2012},
  pages     = {779-784},
  month     = {Oct},
  abstract  = {The process of annotating multimedia contents (such as audio files and image documents) in digital archive requires three types of resource including human, time and money. Today worldwide digital archives have increased in their sizes. As a result, the content annotation would require a large amount of resources. To help reduce the cost associated with the content annotation, we have designed a framework called ARSA (ARchive Social Annotation) to provide a convenient platform for users to annotate digital contents in social networking environment. ARSA adopts the concept of crowdsourcing which allows users to collaborate in content annotation task. The current version of ARSA has been designed and implemented for the Buddhadasa Indapanno Archives (BIA). The contents are in two formats, (1) audio files and (2) image documents of handwritten and typewritten texts. Both contents are related to Buddhadasa's dhamma teaching. ARSA provides a user-friendly and socially connected interface for volunteers to transcribe audio files and image documents. The transcribed texts could be used to implement a search engine for full-text search or used for preparing printed materials.},
  keywords  = {human computer interaction;image retrieval;information retrieval systems;social networking (online);text analysis;user interfaces;ARSA framework;BIA;Buddhadasa Indapanno Archives;Buddhadasa dhamma teaching;archive social annotation tool;audio file transcription;content digitization;cost reduction;crowdsourcing;digital archive;handwritten text image documents;image document transcription;multimedia content annotation;search engine;social networking environment;typewritten text image documents;user-friendly-socially connected interface;Content annotation;Facebook;crowdsourcing;digital archive;social networking;transcription},
}

