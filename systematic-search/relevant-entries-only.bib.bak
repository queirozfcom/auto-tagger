@InProceedings{5486567,
  author    = {R. M. Ewart and J. Betser and S. A. Sutton},
  title     = {Knowledge Management strategy, tools, and technologies for enabling engineering development planning},
  booktitle = {2010 IEEE/IFIP Network Operations and Management Symposium Workshops},
  year      = {2010},
  pages     = {251-257},
  month     = {April},
  abstract  = {This paper reviews the enabling Knowledge Management (KM) tools and technologies that interconnect the multi-organizational activities associated with the system engineering and development planning (DP) of large complex technical programs that deliver operational capabilities. This paper illustrates the challenges associated with this system of systems engineering process, and how KM strategy, architecture, and technology deployment would facilitate higher productivity of multiple stakeholders collaborating in such multi-year efforts. The complex DP process is presented, and the KM requirements and deployment strategy are discussed.},
