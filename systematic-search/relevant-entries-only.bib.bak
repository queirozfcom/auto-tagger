@InCollection{Raieli2013,
  author    = {Roberto Raieli},
  title     = {4 - MIR’s methodology in the context of \{LIS\}},
  booktitle = {Multimedia Information Retrieval},
  publisher = {Chandos Publishing},
  year      = {2013},
  editor    = {Raieli, Roberto},
  series    = {Chandos Information Professional Series},
  pages     = {89 - 132},
  isbn      = {978-1-84334-722-4},
  abstract  = {Abstract: In this chapter, after a quick history of MIR’s principles, a series of clarifications on the state-of-the-art developments and the problems arising from such studies are presented. Some important future directions of study and research are then indicated. With regard to the usefulness of MIR’s methods, it is pointed out how research and experimentation for improving the structure and methodology of systems should always be based on the actual needs of users, and their responses to the new operational proposals. In investigating the actual functionality of \{MIR\} systems, the discussion of the limitations of the contentual objective access to documents is decisive and mathematically objective, as is the discussion of the gap between it and the conceptual-interpretive access, both internationally appointed as the semantic gap. Since the meaning of a multimedia document is rarely explicit, the goal of the system must be to provide support to overcome this gap between the simplicity of content processing offered by the machine and the rich semantic expectations of the user. A suggested solution is integrating into \{MIR\} the method of the ontologies, new guides for navigating the Semantic Web. It is about establishing a truly organic approach to multimedia search, able to take into account univocally the concrete and conceptual, the contentual and semantic representativeness of documents. Finally, accepting as a solution the use of ontologies as being compatible with MIR, to escape their rigours it is hypothetically valid to combine ontologies with folksonomies as systems for the free collaborative content categorization of documents based on labels assigned directly by end-users. The integration between the semantic tools of ontologies and folksonomies, which in turn are integrated with content-based tools, can reconcile the opposition between the principles of semantic-interpretative treatments and contentual objective treatments in the general organic tools of MIR. },
  doi       = {https://doi.org/10.1016/B978-1-84334-722-4.50004-7},
  keywords  = {state of the art, user needs, digital reference, effectiveness, efficiency, human-machine interaction, computer understanding, semantic gap, ontology, folksonomy, semantic web, web 2.0},
  url       = {http://www.sciencedirect.com/science/article/pii/B9781843347224500047},
}

@InCollection{Ballard2012,
  author    = {Terry Ballard},
  title     = {11 - Discovery platforms},
  booktitle = {Google This!},
  publisher = {Chandos Publishing},
  year      = {2012},
  editor    = {Ballard, Terry},
  series    = {Chandos Information Professional Series},
  pages     = {147 - 156},
  isbn      = {978-1-84334-677-7},
  abstract  = {Abstract: Since web catalogs completely eliminated card catalogs in the 1990s there has been little substantive change in the way they work. All of them have the traditional card catalog searches, as well as keyword. There is an option to refine searches to bring in a more focused, manageable set, but extensive research has shown that this option is almost never taken. This chapter shows that as the twenty-first century saw the rise of Google, the expectations of library users changed, with more patrons being dissatisfied with the classic online catalog, which they saw as containing too much library jargon. It looks at the rise of a new type of catalog that overlaid the classic catalog and delivered a more intuitive look. It had tag clouds and facets to limit the results to certain types of material or locations. Since libraries were running both types of catalogs simultaneously, it was easy to find out if they were more likely to limit a search in a discovery catalog. Several libraries reported that users were 20 times more likely to refine a search in a discovery catalog. },
  doi       = {https://doi.org/10.1016/B978-1-84334-677-7.50011-5},
  keywords  = {AquaBrowser, discovery platforms, Encore, OPAC, Summon},
  url       = {http://www.sciencedirect.com/science/article/pii/B9781843346777500115},
}

