  document_type = {Conference Paper},
  doi           = {10.1109/ICDMW.2009.90},
  issn          = {2375-9232},
  journal       = {ICDM Workshops 2009 - IEEE International Conference on Data Mining},
  keywords      = {classification;data mining;distributed processing;groupware;linear programming;peer-to-peer computing;Internet;P2P classifier learning system;TagLearner;collaborative tagging;data mining;distributed linear programming;knowledge extraction;peer-to-peer system;text document classification;Collaboration;Data mining;Distributed computing;Internet;Labeling;Large-scale systems;Learning systems;Peer to peer computing;Software libraries;Tagging},
  source        = {Scopus},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-77951149175&doi=10.1109%2fICDMW.2009.90&partnerID=40&md5=9ee9aebd556b81cfba777030c4a21538},
}

@InProceedings{jing_zeng_2009,
  author          = {Jing, P. and Zeng, D.},
  title           = {Topic-based web page recommendation using tags},
  booktitle       = {2009 IEEE International Conference on Intelligence and Security Informatics},
  year            = {2009},
  pages           = {269-271},
  month           = {June},
  note            = {cited By},
  abstract        = {Collaborative tagging sites allow users to save and annotate their favorite web contents with tags. These tags provide a novel source of information for collaborative filtering. This paper proposes a probabilistic approach to leverage information embedded in tags to improve the effectiveness of Web page recommendation in a social information management context. In our approach, the probability of a Web page visit by a user is estimated by summing up the relevance of this Web page to this user's tags, and then those pages with the highest probabilities are recommended. Experiments using two real-world collaborative tagging datasets show that our algorithms outperform the common collaborative filtering methods. ©2009 IEEE.},
