}

@Conference{bai_etal_2009,
  author        = {Bai, X. and Bertier, M. and Guerraoui, R. and Kermarrec, A.-M.},
  title         = {Toward personalized peer-to-peer top-K processing},
  year          = {2009},
  pages         = {1-6},
  note          = {cited By},
  abstract      = {We present the first personalized peer-to-peer top-k search protocol for a collaborative tagging system. Each peer maintains relevant personalized information about its tagging behavior as well as that of its social neighbors, and uses those to locally process its queries. Extensive experiments based on a real-world dataset crawled from del.icio.us shows that very little storage at each peer suffices to get almost the same results as a hypothetical centralized solution with infinite storage. Copyright 2009 ACM.},
