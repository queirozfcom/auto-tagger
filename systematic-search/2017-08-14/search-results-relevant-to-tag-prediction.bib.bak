  issn      = {1550-445X},
  keywords  = {cloud computing;document handling;hypermedia markup languages;quality of service;DEMODS;DaaSes;HTML documents;automatic service lookup;cloud computing;cloud platforms;data assets;data-as-a-service;description model for DaaS;quality of service;service information;Contracts;Data models;Ontologies;Pricing;Quality of service;Query processing;Web services;Cloud computing;Data-as-a-Service (DaaS);data marketplace;discovery;service and data description},
