  keywords = {pattern classification;social networking (online);trusted computing;defense strategies;knowledge-based social authentication schemes;online social contexts;online social networks;physical social contexts;social authentication applications;social relationships;socially aware authentication schemes;trust-based social authentication schemes;user social interactions;Authentication;Context;Knowledge based systems;Social network services;Taxonomy;Tutorials;Multi-factor authentication;social authentication;social factors;social media;systematic literature review},
