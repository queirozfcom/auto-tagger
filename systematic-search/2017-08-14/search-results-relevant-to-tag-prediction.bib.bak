  keywords  = {protocols;radio networks;radiocommunication;telecommunication security;MNO;generalization approach;mobile network operators;near field communication;real time use location;remote attestation;security;service providers;step-up analysis;trusted NFC application development;trusted-computing protocol;wireless communication technology;Collaboration;Communication system security;Mobile communication;Mobile computing;Mobile handsets;Security;Wireless communication;MNO;Mobile Service;NFC;Wireless Environment},
}

@InProceedings{ketterl_etal_2008,
