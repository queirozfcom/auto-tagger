@InProceedings{tulaboev_oxley_2010,
  author    = {A. Tulaboev and A. Oxley},
  title     = {A pilot study in using web 2.0 to aid academic writing skills},
  booktitle = {2010 IEEE Conference on Open Systems (ICOS 2010)},
  year      = {2010},
  pages     = {45-50},
