  keywords  = {collaborative filtering;matrix decomposition;probability;recommender systems;social networking (online);trusted computing;Delicious;Last.fm;STPMF;cold-start;collaborative filtering approach;data sparsity;neighborhood model;probabilistic matrix factorization;recommendation accuracy;recommendation algorithm;similarity propagation;similarity relationships;social data;social media;social network;trust propagation;trust relationships;user model;Collaboration;Data models;Graphical models;Probabilistic logic;Recommender systems;Social network services;collaborative filtering;matrix factorization model;neighborhood model;similarity propagation;social network;trust propagation},
}

@InProceedings{anwar_etal_2013,
