  keywords  = {audio signal processing;feature extraction;music;neural nets;signal classification;speech recognition;artificial neural system;audio channel;audio content classification;audio feature extraction;audio information;audio semantic analysis;feature difference saliency;feature ranking algorithm;feature selection;feature-based pattern classification;multiple speaker voices;music pattern detection;radio production;stereo difference;stereo information;stereo radio programme;voice pattern detection;Classification algorithms;Feature extraction;Multimedia communication;Multimedia databases;Pattern classification;Semantics;Training;Audio;features;pattern-classification;radio;semantics;stereo},
}

@InProceedings{fan_etal_2010,
  author    = {J. Fan and Y. Shen and N. Zhou and Y. Gao},
  title     = {Harvesting large-scale weakly-tagged image databases from the web},
  booktitle = {2010 IEEE Computer Society Conference on Computer Vision and Pattern Recognition},
  year      = {2010},
  pages     = {802-809},
  month     = {June},
  abstract  = {To leverage large-scale weakly-tagged images for computer vision tasks (such as object detection and scene recognition), a novel cross-modal tag cleansing and junk image filtering algorithm is developed for cleansing the weakly-tagged images and their social tags (i.e., removing irrelevant images and finding the most relevant tags for each image) by integrating both the visual similarity contexts between the images and the semantic similarity contexts between their tags. Our algorithm can address the issues of spams, polysemes and synonyms more effectively and determine the relevance between the images and their social tags more precisely, thus it can allow us to create large amounts of training images with more reliable labels by harvesting from large-scale weakly-tagged images, which can further be used to achieve more effective classifier training for many computer vision tasks.},
