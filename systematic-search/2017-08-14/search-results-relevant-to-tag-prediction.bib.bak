@InProceedings{goh_etal_2013,
  author    = {W. W. Goh and Jer Lang Hong and Kheng Swee Goh},
  title     = {Students' behavior and perception of using Facebook as a learning tool},
  booktitle = {2013 8th International Conference on Computer Science Education},
  year      = {2013},
  pages     = {731-736},
  month     = {April},
  abstract  = {The evolution of World Wide Web technologies since 1990 from Web 1.0 to Web 3.0 have dramatically influenced learning of the students, thus provided a new learning environment beyond those traditionally conducted. However, students in academic institutions, who are categorised as digital native, are familiar in using Web 2.0 tools for their personal life but lack the primary skills to use them academically. This paper addresses the use of one of the most widely known social networking tool, Facebook as a learning tool for higher education students, with emphasis to developing countries such as Malaysia. The purpose of this study is to identify student's perception on using Facebook as a learning tool. Activity theory is adopted as a conceptual framework and analytical tool to demonstrate students' perceptions in using Facebook. Survey was distributed to evaluate on students' perceptions in using Facebook. The outcome of the survey is certainly helpful to determine the future trend of using online social networking tools as a learning tool in higher education.},
  doi       = {10.1109/ICCSE.2013.6554004},
  keywords  = {Internet;computer aided instruction;further education;social aspects of automation;social networking (online);Facebook;Malaysia;Web 1.0;Web 2.0 tools;Web 3.0;World Wide Web technologies;academic institutions;higher education students;learning tool;online social networking tools;students behavior;Communities;Computers;Educational institutions;Facebook;Tutorials;Writing;Activity Theory;Student-Centred Learning;Web 2.0},
}

@InProceedings{che_etal_2013,
  author    = {X. Che and H. Yang and C. Meinel},
  title     = {Tree-structure outline generation for lecture videos with synchronized slides},
  booktitle = {2013 Second International Conference on E-Learning and E-Technologies in Education (ICEEE)},
  year      = {2013},
  pages     = {87-92},
  month     = {Sept},
  abstract  = {In this paper we propose a solution to generate tree-structure outline for lecture videos by analyzing their synchronized slides, by which detailed lecture overview can be automatically provided to E-learning portal users. Starting with OCR (Optical Character Recognition) result, we reconstruct the content of each slide. After that, we explore logical relations between slides, in order to make them hierarchical. And all potential redundant content will also be removed. Our evaluation shows that, based on our test dataset, the final outline achieved retains about 1/4 of the original texts from all slides and is organized well in an up-to-6-level tree structure. Furthermore, the average accuracy of all slide titles, which are undoubtedly the most important information, reaches 86%.},
