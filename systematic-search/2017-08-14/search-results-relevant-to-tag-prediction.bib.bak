  issn      = {1949-3983},
  keywords  = {learning (artificial intelligence);search engines;social networking (online);video retrieval;Flickr images;Internet videos;TRECVID benchmark;YouTube videos;positive training examples;social media sources;socially tagged images;socially tagged videos;source evaluation;strategy evaluation;video concepts learning;video search engine;visual tagging source;Detectors;Media;Semantics;Tagging;Training;Visualization;YouTube},
}

