  keywords  = {Internet;human computer interaction;search engines;social networking (online);user centred design;MANOVA analysis;Web content exchange;Web pages;information interaction;nonwork information task;search engine;social bookmarking services;social navigation function;user perception;user-centered design;work information task;Collaboration;Context;Human computer interaction;Humans;Information science;Navigation;Tagging;information interaction;social bookmarking;social navigation;social tagging},
}

@InProceedings{bouhini_etal_2016,
  author    = {C. Bouhini and M. Géry and C. Largeron},
  title     = {Personalized information retrieval models integrating the user's profile},
  booktitle = {2016 IEEE Tenth International Conference on Research Challenges in Information Science (RCIS)},
  year      = {2016},
  pages     = {1-9},
  month     = {June},
  abstract  = {Personalized Information Retrieval (PIR) exploits the user's data in order to refine the retrieval, like for instance when users with different backgrounds may express different information needs with the same query. However, this additional source of information is not supported by the classical Information Retrieval (IR) process. In order to overcome this limit, we propose to generate the user profile out from his profile and social data. Then, we introduce several Personalized Information Retrieval models which integrate this profile at the querying step, allowing to personalize the search results. We study several combinations of the initial user's query with his profile. Furthermore, we present a PIR test collection that we built from the social bookmarking network Delicious, in order to evaluate our PIR models. Our experiments showed that the PIR models improve the retrieval results.},
  doi       = {10.1109/RCIS.2016.7549310},
  keywords  = {query processing;IR process;PIR;PIR test collection;personalized information retrieval models;querying step;user profile generation;Adaptation models;Androids;Computational modeling;Data models;Humanoid robots;Social network services;Folksonomies;Personalized Information Retrieval;Social Data;User's Profile},
}

@Article{temmermans_etal_2012,
  author   = {F. Temmermans and F. Dufaux and P. Schelkens},
