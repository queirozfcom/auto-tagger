  author    = {S. Wu and Min Jiang and X. Gao and Guiying Wei},
  title     = {Webpage Recommender System concerning high dimensional and sparse features},
  booktitle = {2012 8th International Conference on Information Science and Digital Content Technology (ICIDT2012)},
