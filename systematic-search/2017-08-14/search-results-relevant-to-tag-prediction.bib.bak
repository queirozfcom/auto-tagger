@InProceedings{lee_etal_2009,
  author    = {K. Lee and H. Kim and H. Shin and H. J. Kim},
  title     = {Tag Sense Disambiguation for Clarifying the Vocabulary of Social Tags},
  booktitle = {2009 International Conference on Computational Science and Engineering},
