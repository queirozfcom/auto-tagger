  keywords = {Big Data;multimedia computing;semantic networks;Flickr images;association relation generation;clustering;multimedia big data;multimedia resource organization;retrieval;semantic link network-based model;social tags;surrounding text semantics;tag semantics;Big data;Computational modeling;Data models;Multimedia communication;Ontologies;Semantics;Streaming media;Big data;multimedia resources;multimedia resources organization;semantic link network},
}

