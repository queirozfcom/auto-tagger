  journal         = {IET Conference Publications},
  keywords        = {Collaborative Filtering;Collaborative Tagging;Genre Interestingness Measure;Recommender Systems},
  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84907553903&doi=10.1049%2fcp.2012.2537&partnerID=40&md5=b3dd86eaa99e03f29ab3242a1c28dd06},
}

@Conference{tian_etal_2011,
