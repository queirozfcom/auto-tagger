@Article{wu_etal_2015a,
  author   = {Xindong Wu and Huanhuan Chen and Gongqing Wu and Jun Liu and Qinghua Zheng and Xiaofeng He and Aoying Zhou and Zhong-Qiu Zhao and Bifang Wei and Ming Gao and Yang Li and Qiping Zhang and Shichao Zhang and Ruqian Lu and Nanning Zheng},
