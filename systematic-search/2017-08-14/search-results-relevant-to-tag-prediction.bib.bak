@InProceedings{moghaddam_elahian_2014,
  author    = {M. G. Moghaddam and A. Elahian},
  title     = {A novel temporal trust-based recommender system},
  booktitle = {2014 22nd Iranian Conference on Electrical Engineering (ICEE)},
