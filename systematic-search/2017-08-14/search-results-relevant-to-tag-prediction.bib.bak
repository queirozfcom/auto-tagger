  keywords  = {Web services;natural language processing;ontologies (artificial intelligence);query processing;NLP;Web services;annotation;automatic functional-goals tagging approach;goal-driven semantic service discovery algorithm;keyword-based service discovery;natural language processing;ontology-based service discovery;semantic information;semantic techniques;topic-based service discovery;user query;Data mining;Google;Ontologies;Semantics;Tagging;Videos;Web services;automatic tagging;functional-goal;goal-driven semantic service discovery;web service},
}

