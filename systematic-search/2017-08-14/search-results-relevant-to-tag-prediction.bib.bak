  keywords = {mobile computing;recommender systems;social sciences computing;SARVE;conference navigator;context-aware mobile recommendation services;distributed community detection;smart conference participation;socially aware recommendation;venue recommendation algorithm;Communities;Context;Correlation;Mobile communication;Recommender systems;Schedules;Social network services;Conference participants;context;recommender systems;smart conference;social awareness},
