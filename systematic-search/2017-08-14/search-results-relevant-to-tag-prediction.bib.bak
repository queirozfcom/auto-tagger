@Article{bi_kwok_2014,
  author   = {W. Bi and J. T. Kwok},
  title    = {Mandatory Leaf Node Prediction in Hierarchical Multilabel Classification},
  journal  = {IEEE Transactions on Neural Networks and Learning Systems},
  year     = {2014},
  volume   = {25},
