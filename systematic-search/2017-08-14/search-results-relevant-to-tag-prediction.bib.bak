@InProceedings{trung_etal_2013,
  author    = {D. N. Trung and J. J. Jung and Le Anh Vu and A. Kiss},
  title     = {Towards modeling fuzzy propagation for sentiment analysis in online social networks: A case study on TweetScope},
  booktitle = {2013 IEEE 4th International Conference on Cognitive Infocommunications (CogInfoCom)},
