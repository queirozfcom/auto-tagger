}

@Conference{zoller_etal_2015,
  author          = {Zoller, D. and Doerfel, S. and Jäschke, R. and Stumme, G. and Hotho, A.},
  title           = {On publication usage in a social bookmarking system},
  year            = {2015},
  note            = {cited By},
  abstract        = {Scholarly success is traditionally measured in terms of citations to publications. With the advent of publication management and digital libraries on the web, scholarly usage data has become a target of investigation and new impact metrics computed on such usage data have been proposed - so called altmetrics. In scholarly social bookmarking systems, scientists collect and manage publication meta data and thus reveal their interest in these publications. In this work, we investigate connections between usage metrics and citations, and find posts, exports, and page views of publications to be correlated to citations. Copyright is held by the owner/author(s).},
  affiliation     = {Data Mining and Information Retrieval Group, University of Würzburg, Germany; ITeG, Knowledge and Data Engineering (KDE) Group, University of Kassel, Germany; L3S Research Center, Germany},
