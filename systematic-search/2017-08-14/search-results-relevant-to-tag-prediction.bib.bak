  keywords  = {explosives;knowledge management;law administration;national security;query formulation;recommender systems;terrorism;HOMER project;adaptive knowledge retrieval;counter-terrorism;domain-specific recommendations;folksonomies;homemade explosives;knowledge management;law enforcement;personalized systems;security;semantic search;Explosives;Knowledge management;Organizations;Recommender systems;Semantics;Tagging;Vocabulary;adaptive knowledge retrieval;folksonomy;personalization;recommender systems;result precision;semantic enrichment},
}

@InProceedings{balaid_etal_2014,
  author    = {A. Balaid and M. Z. Abd Rozan and S. N. Abdullah},
  title     = {Influential factors of knowledge maps adoption in software development organizations: A pilot case study},
  booktitle = {2014 8th. Malaysian Software Engineering Conference (MySEC)},
  year      = {2014},
  pages     = {201-205},
  month     = {Sept},
  abstract  = {Nowadays, the most important phenomenon in software development organizations is their ability to make the available knowledge internally visible in a formal way. Most organizations are facing difficulty in making important knowledge visible as part of their knowledge management effort. Knowledge maps make it easier to locate this knowledge, capture it, and make it visible for the organization. While there has been much research on knowledge maps techniques and tools, there has been little empirical research into the factors that affect knowledge maps adoption. Therefore, through a pilot case study of knowledge maps adoption in software organizations, this study identifies the key factors that influencing the adoption of knowledge maps. Practitioners and researchers involved in knowledge maps may find this paper useful, since it provides better understanding of the important factors that affect knowledge maps adoption.},
  doi       = {10.1109/MySec.2014.6986014},
  keywords  = {knowledge management;software development management;knowledge management effort;knowledge map adoption;software development organizations;Companies;Context;Interviews;Meteorology;Semantics;Software;factors influence;knowledge maps;knowledge maps adoption},
