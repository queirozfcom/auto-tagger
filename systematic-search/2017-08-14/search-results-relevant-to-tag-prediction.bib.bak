  keywords  = {data mining;sampling methods;social networking (online);DBLP;information mining;link prediction;snowball sampling;sparse egocentric networks;user profile modeling process;user social network information;Buildings;Communities;Context;Data mining;Social network services;Sociology;Statistics;User modeling;link prediction;snowball sampling;social network analysis},
}

