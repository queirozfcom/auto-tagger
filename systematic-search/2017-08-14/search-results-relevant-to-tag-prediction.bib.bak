}

@Conference{park_etal_2015,
  author          = {Park, J. and Park, K.W. and Yun, Y. and Kim, M. and Rho, S. and Man, K.L. and Chong, W.K.},
  title           = {Efficient multimedia contents management system with tag recommendations},
  year            = {2015},
  volume          = {2},
  pages           = {737-738},
  note            = {cited By},
  abstract        = {Recently, rapid development of internet technology, the amount of data on the web is exponentially generated. Because of that, users are difficult to find desired multimedia data. For efficient search and management of multimedia data, collaborative tagging is appropriated to describe and categorize the data. Then we proposed the collaborative multimedia management systems based on tag recommendation. Our system suggests the tag candidates for describing and categorizing the multimedia contents.},
  affiliation     = {Department of Multimedia, Sungkyul University, Anyang, South Korea; Computer Science and Software Engineering, Xi'an Jiaotong-Liverpool University, China; International Business School Suzhou (IBSS), Xi'an Jiaotong-Liverpool University, China},
