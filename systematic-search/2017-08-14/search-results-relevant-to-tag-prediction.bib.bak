@Conference{bianchi_etal_2009,
  author          = {Bianchi, M. and Draoli, M. and Gambosi, G. and Pazienza, M.T. and Scarpato, N. and Stellato, A.},
  title           = {ICT tools for the discovery of semantic relations in legal documents},
