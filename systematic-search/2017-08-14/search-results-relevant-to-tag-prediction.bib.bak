@Article{godoy_etal_2014,
  author   = {D. Godoy and G. Rodriguez and F. Scavuzzo},
  title    = {Leveraging Semantic Similarity for Folksonomy-Based Recommendation},
  journal  = {IEEE Internet Computing},
  year     = {2014},
