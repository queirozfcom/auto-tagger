  keywords  = {image annotation;image retrieval;language translation;RIA;annotation accuracy improvement;annotation order matters;arbitrary length image tagging task;automatic image annotation;human annotate images;image annotation datasets;image annotation task;image captioning;image contents;large-scale image management;large-scale image retrieval;learning image-tag correlation;machine translation;recurrent image annotator;sequence generation problem;Correlation;Feature extraction;Indexes;Predictive models;Recurrent neural networks;Training},
}

