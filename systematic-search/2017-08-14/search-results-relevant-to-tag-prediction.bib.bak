  keywords  = {collaborative filtering;computer games;recommender systems;GAMBIT;Movie Lens;collaborative filtering technique;game recommender system;genre-based filter;mean absolute error;prediction accuracy;prediction algorithm;recommendation quality;shared rated games;user-based collaborative filtering;working Web based system;Games;Motion pictures;collaborative filtering;games genre;recommendation system},
}

@InProceedings{kardan_etal_2012,
