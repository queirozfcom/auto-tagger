  journal         = {2013 International Conference on Recent Trends in Information Technology, ICRTIT 2013},
  keywords        = {Internet;formal concept analysis;learning (artificial intelligence);query processing;FCA;Web 2.0;artificial intelligence techniques;collaborative tagging system;data mining;formal concept analysis;information retrieval system;lattice structure;machine learning;online document;resource annotation;resource management systems;semantic Web;user query;Collaboration;Formal concept analysis;Lattices;Search problems;Semantics;Tagging;Collaborative tagging;Formal Concept Analysis;folksonomy;information retrieval},
  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84904211592&doi=10.1109%2fICRTIT.2013.6844171&partnerID=40&md5=b123637ef341325acbd3682bf036904d},
}

@Conference{cai_etal_2012,
  author          = {Cai, Y. and Han, H. and Chen, J. and Shao, Y. and Leung, H.-F. and Min, H.},
  title           = {Integrating tags and ratings into user profiling for personalized search in collaborative tagging systems},
  booktitle       = {2012 IEEE/WIC/ACM International Conferences on Web Intelligence and Intelligent Agent Technology},
