  keywords        = {Internet;content-based retrieval;groupware;recommender systems;Web 2.0;collaborative tagging systems;content based information extraction;latent correlation;multiway analysis;quality improvement;recommender systems;three-mode tensors;Collaboration;Matrix decomposition;Ontologies;Recommender systems;Semantics;Tagging;Tensile stress;Electronic commerce;Semantic Web;World Wide Web;recommender systems},
  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-79959607296&doi=10.1109%2fTSMCA.2011.2132708&partnerID=40&md5=cb2d7e0f89d2f29c362f8495e2a6b2e3},
}

