  keywords  = {information retrieval;social networking (online);Web 2.0;folksonomy system;social tagging system semantics;user cognitive information;Electronic publishing;Encyclopedias;Internet;Navigation;Semantics;Thesauri},
}

@InProceedings{noor_etal_2015,
  author    = {F. Noor and A. Shah and S. A. Khan},
  title     = {Relation mining using cross correlation of multi domain social networks},
  booktitle = {2015 SAI Intelligent Systems Conference (IntelliSys)},
  year      = {2015},
  pages     = {898-903},
  month     = {Nov},
  abstract  = {Social network analysis has emerged as an important research area in intelligence analysis to facilitate decision-maker in more informed decisions. With the passage of time social network services are increasingly being used in legal and criminal investigations including understanding and tracking of dark networks, i.e., illegal and covert networks. Criminal organizations are well-suited to be studied using social network analysis as they consist of networks of individuals that span countries and continents using false identities over different networks to remain anonymous. There is a need to recognize the criminals carrying different fake identities to correctly track their immoral activities. This research article is focusing on devising a method which can be used to identify group of people i.e. criminal, terrorist or friends on different Social networks by analyzing patterns and performing correlations across different social network systems. Data integration using cross correlation is used to merge entities to form single fused networks. This single fused network also estimate the intensity of relationship between individuals in general networks based on the number of networks they are connected. To best of our knowledge no such studies is done in the social network analysis, therefore this method is unique and offers innovations in making effective use of presence of people in different networks.},
  doi       = {10.1109/IntelliSys.2015.7361250},
  keywords  = {data integration;data mining;social networking (online);criminal investigation;cross correlation;data integration;intelligence analysis;legal investigation;multidomain social networks;pattern analysis;relation mining;single fused networks;social network analysis;time social network services;Cleaning;Correlation;Data mining;Feature extraction;Intelligent systems;Social network services;Terrorism;Actor Identification;Fused Network Formation;Relation Mining;Social Network Analysis},
}

