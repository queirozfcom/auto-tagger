  keywords      = {information retrieval;meta data;multimedia computing;natural language processing;social networking (online);CLSMR;Chinese Korean cross language social media retrieval;Korean language;Korean media files;Wikipedia;YouTube retrieval;collaborative tagging;cross language social media retrieval system;foreign language tagged media content retrieval;human written summary;metadata tags;pattern based software translator;query translation;social media websites;tag translation;Dictionaries;Encyclopedias;Google;Internet;Media;Tagging;YouTube},
  source        = {Scopus},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-80053159799&doi=10.1109%2fIRI.2011.6009561&partnerID=40&md5=1a1b41ac077ccbaec727ff83f2af3af5},
