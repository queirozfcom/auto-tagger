  keywords        = {Internet;classification;indexing;natural language processing;query processing;social networking (online);Delicious2;Flickr1;WordNet;collaborative tagging;data sparsity;folksonomies;information extraction;semantic relationship;social resource sharing system;user queries;Collaboration;Computer science;Content based retrieval;Data mining;Information retrieval;Intelligent systems;Internet;Resource management;Tagging;Taxonomy;WordNet;folksonomies;information retrieval;tag},
  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-77952619035&doi=10.1109%2fICCAE.2010.5451821&partnerID=40&md5=182dcecc671561340c9b6ed8fef030bf},
