@InProceedings{saghiri_bagheri_2009,
  author    = {A. Saghiri and A. Bagheri},
  title     = {An Adaptive Architecture for Personalized Search ?Engine in Ubiquitous Environment with Peer to Peer Systems},
  booktitle = {2009 International Conference on Information and Multimedia Technology},
  year      = {2009},
  pages     = {107-111},
  month     = {Dec},
  abstract  = {Future ubiquitous environments will be emerged over many of the computers that are dynamically networked. The personalized data collected by ubiquitous environment provide important information for ubiquitous applications. This type of application will form a peer to peer system over the Internet to offer common services. Providing architecture for large-scale personalized peer to peer information retrieval, e. g. personalized searching of the ubiquitous environment is an appealing application for peer to peer systems. In this paper, we propose an adaptive architecture to structure peer to peer systems. An adaptive architecture for personalized information retrieval considering user's profile is important. We present the first architecture for personalized information retrieval in ubiquitous environment with peer-to-peer systems. This paper introduces a novel overlay design architecture, called AIR (adaptive information retrieval), for efficient, scalable, collaborative, adaptive and personalized information retrieval.},
