  keywords  = {recommender systems;social networking (online);friend recommender system architecture;friend recommender system design;social bookmarking domain;social environments;social media systems;social network;social recommender systems;user recommendation;Computer architecture;Design methodology;Media;Recommender systems;Tagging;Twitter;Friend Recommendation;Social Bookmarking;Tagging System},
}

@InProceedings{rathi_given_2010,
