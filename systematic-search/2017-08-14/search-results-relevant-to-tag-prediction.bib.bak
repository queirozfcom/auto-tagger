@InCollection{ortega_2016,
  author    = {José Luis Ortega},
  title     = {Chapter 4 - Reference Management Tools},
  booktitle = {Social Network Sites for Scientists},
  publisher = {Chandos Publishing},
  year      = {2016},
  editor    = {Ortega, José Luis},
  pages     = {65 - 99},
  isbn      = {978-0-08-100592-7},
  abstract  = {Abstract This chapter analyses the bibliographic coverage, networking functions and population distribution of reference management tools. These networking spaces generally emerged from desktop applications for organizing bibliographic references to social networking environments setup to share and tag citations for scholars. Two of the most important reference management tools, Mendeley and Zotero, are described in detail. Mendeley has the largest bibliographic database where users can read and tag the publications that other members share in the public catalogue. However, its networking functionalities are hardly exploited at all, which casts serious doubts on its future as a social networking platform. Zotero, on the other hand, is a local, reduced platform occupied above all by American users from the Humanities and Social Sciences. Nevertheless, the great limitation of this platform is that it does not have a public catalogue of the references added to the service, which impedes the sharing of references between members. As a result of this users employ groups as the means to achieve collaboration. },
