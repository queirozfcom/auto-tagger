  journal         = {Proceedings of the 2013 International Conference on Pattern Recognition, Informatics and Mobile Engineering, PRIME 2013},
  keywords        = {Internet;Web sites;data mining;fuzzy set theory;pattern clustering;rough set theory;Web 2.0 Web site;Web 2.0 social bookmark selection;Web mining;collaborative tagging service;data mining;feature selection technique;marginal bookmarked URL subset;rough K-means algorithm;social book marking Web site;soft rough fuzzy clustering;unsupervised quick reduct feature selection algorithm;user generated tag clustering;user-generated phrase;user-generated word;Algorithm design and analysis;Clustering algorithms;Indexes;Informatics;Pattern recognition;Tagging;Web 2.0;Bookmark selection;Clustering;Fuzzy soft Rough clustering;Rough K means;Unsupervised Quick Reduct},
