  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84869101199&partnerID=40&md5=125ac19cf6a76fab474d9af6930f690b},
}

@Conference{yahia_etal_2008,
  author        = {Yahia, S.A. and Benedikt, M. and Lakshmanan, L.V.S. and Stoyanovichy, J.},
  title         = {Efficient network aware search in collaborative tagging sites},
  year          = {2008},
  volume        = {1},
  number        = {1},
  pages         = {710-721},
  note          = {cited By},
  abstract      = {The popularity of collaborative tagging sites presents a unique opportunity to explore keyword search in a context where query results are determined by the opinion of a network of taggers related to a seeker. In this paper, we present the first in-depth study of network-aware search. We investigate efficient top-k processing when the score of an answer is computed as its popularity among members of a seeker's network. We argue that obvious adaptations of top-k algorithms are too space-intensive, due to the dependence of scores on the seeker's network. We therefore develop algorithms based on maintaining score upper-bounds. The global upper-bound approach maintains a single score upper-bound for every pair of item and tag, over the entire collection of users. The resulting bounds are very coarse. We thus investigate clustering seekers based on similar behavior of their networks. We show that finding the optimal clustering of seekers is intractable, but we provide heuristic methods that give substantial time improvements. We then give an optimization that can benefit smaller populations of seekers based on clustering of taggers. Our results are supported by extensive experiments on del.icio.us datasets. © 2008 VLDB Endowment.},
  affiliation   = {Yahoo Research, United States; Oxford University, United Kingdom; University of British Columbia, Canada; Columbia University, United States; Natural Sciences and Engineering Research Council of Canada, Canada; National Institute of Health, United States},
  document_type = {Article},
  journal       = {Proceedings of the VLDB Endowment},
  source        = {Scopus},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84859178171&partnerID=40&md5=7ae1d5e57255f5c387a9e3289fd450e1},
}

@Conference{chow_etal_2008,
  author          = {Chow, K.O. and Chan, A.Y.K. and Cheung, S.K.S. and Wong, G.T.L.},
  title           = {Collaborative tagging in a mobile synchronized lecture environment},
  year            = {2008},
  pages           = {189-193},
  note            = {cited By},
  abstract        = {Teaching and learning as two educational activities are centrally important in education quality. However, the two often occur in separate timing and in the implied ordering of teaching first and learning happens afterwards. This paper proposes a concurrent teaching and learning approach via synchronization of the slide presentation by teacher and tag addition by students. Better learning is attained when students increase their active learning with tagging at the same time that students are in attendance of lecture. The synchronization is AJAX-enabled.},
  affiliation     = {City University of Hong Kong, Kowloon Tong, Hong Kong, Hong Kong; Hong Kong Baptist University, Kowloon, Hong Kong, Hong Kong; Chinese University of Hong Kong, Shatin, Hong Kong, Hong Kong},
  author_keywords = {Collaborative tagging; Mobile learning; Web 2.0},
