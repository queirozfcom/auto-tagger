  journal       = {Grid Computing Environments Workshop, GCE 2008},
  keywords      = {Internet;identification technology;information retrieval;Internet;collective collaborative tagging system;folksonomy analysis;information discovery;information search;keyword tag;online system;service architecture;service provider;tag-based folksonomy;Algorithm design and analysis;Collaboration;Information analysis;Laboratories;Portals;Service oriented architecture;Social network services;Tagging;Uniform resource locators;Web and internet services},
  source        = {Scopus},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-62949119195&doi=10.1109%2fGCE.2008.4738442&partnerID=40&md5=033ab345641da89408ebabdb482e391a},
