}

@Conference{klasnja-milicevic_etal_2012,
  author          = {Klašnja-Milićevic, A. and Vesin, B. and Ivanović, M. and Budimac, Z.},
  title           = {Personalisation of programming tutoring system using tag-based recommender systems},
  booktitle       = {2012 IEEE 12th International Conference on Advanced Learning Technologies},
