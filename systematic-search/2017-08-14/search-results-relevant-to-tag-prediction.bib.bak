  keywords  = {knowledge based systems;literature;recommender systems;background knowledge;information overload;knowledge gap;literature resource;personalized paper recommendation;research paper recommendation;target knowledge;Collaboration;Correlation;Filtering;Ontologies;Proposals;Semantics;background knowledge;concept map;knowledge gap;literature recommendation},
}

