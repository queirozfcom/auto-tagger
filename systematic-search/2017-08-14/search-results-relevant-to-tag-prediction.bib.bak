  keywords  = {cache storage;critical path analysis;pattern classification;protocols;shared memory systems;MOESI cache-coherence protocol;NUMA systems;PARSEC;SPLASH-2;STAP;automatic sharing classification;cache-coherent systems;critical-path communication latency;data sharing patterns;inter-socket directory-based shared memory networks;intra-node directory-based shared memory networks;sharing/timing adaptive push;Benchmark testing;Coherence;Data transfer;Hardware;Prefetching;Protocols;Timing},
}

@InProceedings{liu_etal_2012,
