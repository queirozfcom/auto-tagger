  affiliation     = {EEDIS Laboratory, Djillali Liabes University of Sidi Bel Abbes, Sidi Bel Abbes, Algeria},
  author_keywords = {Collaborative tagging; Folksonomies; Fuzzy clustering; Ontologies; Similarity measure},
