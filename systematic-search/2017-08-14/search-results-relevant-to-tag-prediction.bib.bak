  journal         = {Proceedings of the 2015 IEEE 19th International Conference on Computer Supported Cooperative Work in Design, CSCWD 2015},
  keywords        = {Web sites;groupware;Web pages;conceptual modeling;folksonomy;hared conceptualization;tripartite model;Collaborative Conceptual Modeling;Collaborative Tagging;Controlled Experiments;Data Extraction;Folksonomies;Shared Conceptualization},
