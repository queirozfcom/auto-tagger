  keywords  = {IEEE standards;information analysis;information retrieval;thesauri;FER;Faculty of Electrical Engineering and Computing;IEEE Journals and Magazines;IEEE terms;IEEE-IET Electronic Library;Mendeley academic social network;author keywords;controlled vocabulary;information retrieval function;scientific papers;social tags;user tags;Air gaps;Correlation;Databases;Magnetic fields;Permanent magnet motors;Thesauri;Vocabulary},
}

@InProceedings{ungrangsi_anutariya_2011,
  author    = {R. Ungrangsi and C. Anutariya},
  title     = {A comparison study of Flickr's folksonomies and ontologies},
  booktitle = {2011 Eighth International Joint Conference on Computer Science and Software Engineering (JCSSE)},
  year      = {2011},
  pages     = {257-262},
  month     = {May},
  abstract  = {Flickr is a widely-known Web 2.0 photo sharing system that allows users to describe their own photos with tags (aka. folksonomies) for indexing purposes. However, its tag-based photo retrieval function is severely hampered by the inherent nature of folksonomies. As the first step moving towards the alleviation of the problem, this paper, therefore, investigates characterizations of folksonomies on Flickr through its all-time most popular tags. In particular, the study focuses on tag clusters, related tags, and tag categorizations of the all-time most popular tags and compares them with concepts formally defined in ontologies from the Semantic Web. The experimental results imply the feasibilit of employing the Semantic Web technology in searching Flickr photos.},
  doi       = {10.1109/JCSSE.2011.5930131},
  keywords  = {classification;image retrieval;indexing;ontologies (artificial intelligence);semantic Web;social networking (online);Flickr;Web 2.0 photo sharing system;folksonomies;indexing;ontologies;semantic Web;tag categorizations;tag clusters;tag-based photo retrieval function;Semantic Web;Web2.0;folksonomy;ontology},
}

@InProceedings{abdullah_etal_2010,
  author    = {N. Abdullah and Y. Xu and S. Geva and J. Chen},
  title     = {Infrequent Purchased Product Recommendation Making Based on User Behaviour and Opinions in E-commerce Sites},
  booktitle = {2010 IEEE International Conference on Data Mining Workshops},
  year      = {2010},
  pages     = {1084-1091},
  month     = {Dec},
  abstract  = {Web based commercial recommender systems (RS) can help users to make decisions about which product to purchase from the vast amount of products available on the Internet. Currently, many commercial recommender systems are developed for recommending frequently purchased products where a large amount of explicit ratings or purchase history data is available to predict user preferences. However, for products that are infrequently purchased by users, it is difficult to collect such data and, thus, user profiling becomes a major challenge for recommending these kinds of products. This paper proposes a recommendation approach for infrequently purchased products based on user opinions and navigation data. User opinion data, which is collected from product review data, is used to generate product profiles and user navigation data is used to generate user profiles, both of which are used for recommending products that best satisfy the users' needs. Experiments conducted on real e-commerce data show that the proposed approach, named, Adaptive Collaborative Filtering (ACF), which utilizes user and product profiles, outperforms the Query Expansion (QE) approach that only utilizes product profiles to recommend products. The ACF also performs better than the Basic Search (BS) approach, which is widely applied by the current e-commerce applications.},
  doi       = {10.1109/ICDMW.2010.116},
  issn      = {2375-9232},
  keywords  = {Internet;Web sites;consumer behaviour;customer profiles;data handling;decision making;electronic commerce;information filtering;query processing;recommender systems;ACF;Internet;Web based commercial recommender system;adaptive collaborative filtering;data collection;decision making;e-commerce sites;infrequent purchased product recommendation making;product profile;query expansion;user behaviour;user navigation data;user opinion data;user profile generation;collaborative filtering;product profiling;recommender system;user navigation and behaviour data;user profiling},
}

@InProceedings{chiu_etal_2015,
  author    = {P. S. Chiu and C. C. Kao and Y. H. Pu and P. F. Lo and Y. M. Huang},
  title     = {The Development of a Decision Support System for Successful Mobile Learning},
  booktitle = {2015 IEEE 15th International Conference on Advanced Learning Technologies},
  year      = {2015},
  pages     = {114-115},
  month     = {July},
  abstract  = {Mobile learning is potential and valuable in education. However, most of previous developing strategies of m-learning were analyzed by means of individual knowledge, experience, judgment, and statistic software. Although these methods provide systematic steps for the implementation of m-learning strategies, the correlation between factors cannot be considered thoroughly through the steps, and it is generally necessary to depend on analyzers' capability and experience to select strategic factors and the results of judgment. Hence, there are methodological limits. Fuzzy analytic hierarchy process methods were used to analyze 89 valid questionnaires in this case study. The evaluation indexes of the successful factors of m-learning were employed to transfer the opinions of each interviewed learner. The relative weight of each evaluation index was calculated, and the entire weight system of the indexes was established, which was then used as the strategic tool for m-learning development.},
  doi       = {10.1109/ICALT.2015.112},
  issn      = {2161-3761},
  keywords  = {analytic hierarchy process;decision support systems;fuzzy set theory;mobile learning;decision support system;evaluation index;fuzzy analytic hierarchy process methods;m-learning;successful mobile learning;weight system;Computers;Conferences;Electronic learning;Indexes;Mobile communication;decision support system;fuzzy analytic hierarchy process;mobile learning},
}

