@InProceedings{samarawickrama_etal_2015,
  author    = {S. Samarawickrama and S. Karunasekera and A. Harwood},
  title     = {Finding High-Level Topics and Tweet Labeling Using Topic Models},
  booktitle = {2015 IEEE 21st International Conference on Parallel and Distributed Systems (ICPADS)},
  year      = {2015},
  pages     = {242-249},
  month     = {Dec},
  abstract  = {Making sense of Twitter data streams is challenging due to the extremely high volume of data. One way to address this challenge is to consider these data streams as containing a set of high-level topics. In this research we address the problem of: given a collection of tweets about K high-level topics, how to find topic words that describe these topics as well as how to label each tweet with one of the K topics using a topic modeling approach. Current research has shown that applying topic modeling algorithms directly on tweets does not lead to good results. Hence one approach is to group related tweets together, so as to form a single “pseudo-document”, which is more informative than a single tweet. In this paper we evaluate different grouping schemes found in the literature and propose a new grouping scheme utilizing named entities and word collocations. Results show that our proposed scheme performs better than the existing approaches, to a some extent for all the test cases, and for both finding high-level topics and tweet labeling.},
  doi       = {10.1109/ICPADS.2015.38},
  keywords  = {social networking (online);text analysis;word processing;Twitter data streams;grouping scheme;grouping schemes;high-level topic model;named entities;pseudodocument;topic modeling algorithms;tweet labeling;word collocations;Blogs;Distributed databases;Labeling;Media;Resource management;Tagging;Twitter;Twitter;content-analysis;microblogging;social media mining;topic-modeling},
}

@InProceedings{wang_etal_2014,
  author    = {J. Wang and Z. Liu and H. Zhao},
  title     = {Group Recommendation Using Topic Identification in Social Networks},
  booktitle = {2014 Sixth International Conference on Intelligent Human-Machine Systems and Cybernetics},
  year      = {2014},
  volume    = {1},
  pages     = {355-358},
  month     = {Aug},
  abstract  = {Many recommendation systems recommend item (book, music, news, or restaurant) to a group of users with the help of social network services like micro-blogging, which are called as group recommendation systems. However, as the social network services always contain large amounts of information on different topics, and people have different influence on different topic, so the group recommendation systems should take the topic identification in large-scale social networks into account to get an appropriate recommendation. In this paper, we proposed a new group recommendation method, which combines topic identification and social networks for group recommendation. In detail, we firstly identify different topical sub-groups by topics in social networks. Secondly, different user factors are used to calculate the user influence (including individual and social) on the topical sub-groups, which can depict the topical sub-group characteristics in different points of view. Experimental results demonstrate that the proposed method can improve the prediction accuracy of the group recommendation.},
  doi       = {10.1109/IHMSC.2014.93},
  keywords  = {recommender systems;social networking (online);group recommendation systems;large-scale social networks;microblogging;topic identification;user influence;Accuracy;Blogs;Conference proceedings;Educational institutions;Motion pictures;Prediction algorithms;Social network services;group recommendation;social networks;topic identification},
}

@InProceedings{wu_guo_2012,
