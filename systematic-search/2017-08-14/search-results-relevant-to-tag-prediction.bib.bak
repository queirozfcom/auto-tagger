@InProceedings{lo_etal_2013,
  author    = {S. L. Lo and S. D. Mei and V. Liew},
  title     = {Use of Semantic Co-relation in Target Audience Profiling},
  booktitle = {2013 Fourth Global Congress on Intelligent Systems},
  year      = {2013},
