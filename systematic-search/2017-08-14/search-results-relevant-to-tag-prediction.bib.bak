  keywords = {particle swarm optimisation;recommender systems;social networking (online);PSO-based algorithm;PageRank-based algorithm;celebrity effect;microblog network;microblog user recommendation;particle swarm optimization;social factors;zombie fans effect;Coherence;Fans;Particle swarm optimization;STEM;Social network services;Sociology;Statistics;Microblog social network;particle swarm optimization;user influence;user recommendation},
