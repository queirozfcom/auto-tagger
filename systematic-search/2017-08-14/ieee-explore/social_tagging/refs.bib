month={Aug},}
@ARTICLE{7194815,
author={D. Rafailidis and A. Nanopoulos},
journal={IEEE Transactions on Systems, Man, and Cybernetics: Systems},
title={Modeling Users Preference Dynamics and Side Information in Recommender Systems},
