@INPROCEEDINGS{7423069,
author={H. Zhao and S. Wang and Q. Chen and J. Cao},
booktitle={2015 IEEE Conference on Collaboration and Internet Computing (CIC)},
title={Probabilistic Matrix Factorization Based on Similarity Propagation and Trust Propagation for Recommendation},
year={2015},
