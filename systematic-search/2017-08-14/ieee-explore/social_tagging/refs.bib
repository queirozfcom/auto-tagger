@ARTICLE{5733421,
author={H. Y. Lo and J. C. Wang and H. M. Wang and S. D. Lin},
journal={IEEE Transactions on Multimedia},
title={Cost-Sensitive Multi-Label Learning for Audio Tag Annotation and Retrieval},
