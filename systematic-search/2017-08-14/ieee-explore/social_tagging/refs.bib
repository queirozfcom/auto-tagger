@INPROCEEDINGS{5602873,
author={S. C. I. Li and Y. Sato and H. Nagai and T. Kameoka},
booktitle={Proceedings of SICE Annual Conference 2010},
title={Conceptualization and implementation of food user experience technologies},
year={2010},
pages={2919-2924},
abstract={Studies on food culture have been flourished in recent decades. Although diverse dimensions of food related issues have been argued by researchers from various fields in an interdisciplinary manner, a sense of taste that should be a critical component of food selection and consumption has not been so much discussed. A sense of taste is the most ancient and familiar to everyday life of ordinary people, it is also the last domain concerning the last mile to human habitual world where the past 20 years of rapid growth of ICT could have influence on. We aim to build "Food User Experience Technologies" which is the overall name of the movement to weave and rebuild user experience of food with latest cutting edge technologies, including eTaste (using optical-sensing techniques to measure the different taste sense thresholds for individual sense in different regions/cultures), Food Knowledge Wiki and Food Therapeutic APIs.},
keywords={food products;food technology;cutting edge technology;e-taste;food culture;food knowledge Wiki;food therapeutic;food user experience technology;human habitual world;Collaboration;Cultural differences;Humans;Media;Space technology;Standardization;Visualization;User experience;eCulture;food culture;participatory architecture;taste;user experience technology (key words)},
