@ARTICLE{6589200,
author={W. Lu and J. Li and T. Li and W. Guo and H. Zhang and J. Guo},
journal={IEEE Transactions on Multimedia},
title={Web Multimedia Object Classification Using Cross-Domain Correlation Knowledge},
year={2013},
