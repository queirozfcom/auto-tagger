doi={10.1109/JSEN.2013.2281197},
ISSN={1530-437X},
month={April},}
@INPROCEEDINGS{6738887,
author={W. Xie and Z. Lu and Y. Peng and J. Xiao},
booktitle={2013 IEEE International Conference on Image Processing},
title={Multimodal semi-supervised image classification by combining tag refinement, graph-based learning and support vector regression},
year={2013},
