@ARTICLE{Gemmell2008196,
author={Gemmell, J. and Shepitsen, A. and Mobasher, B. and Burke, R.},
title={Personalizing navigation in folksonomies using hierarchical tag clustering},
journal={Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
year={2008},
volume={5182 LNCS},
pages={196-205},
doi={10.1007/978-3-540-85836-2_19},
note={cited By },
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-52949154343&doi=10.1007%2f978-3-540-85836-2_19&partnerID=40&md5=8ccb5cd82cdac4644d0405fef47b64d3},
affiliation={Center for Web Intelligence, School of Computing, DePaul University, Chicago, IL, United States},
abstract={The popularity of collaborative tagging, otherwise known as "folksonomies", emanate from the flexibility they afford users in navigating large information spaces for resources, tags, or other users, unencumbered by a pre-defined navigational or conceptual hierarchy. Despite its advantages, social tagging also increases user overhead in search and navigation: users are free to apply any tag they wish to a resource, often resulting in a large number of tags that are redundant, ambiguous, or idiosyncratic. Data mining techniques such as clustering provide a means to overcome this problem by learning aggregate user models, and thus reducing noise. In this paper we propose a method to personalize search and navigation based on unsupervised hierarchical agglomerative tag clustering. Given a user profile, represented as a vector of tags, the learned tag clusters provide the nexus between the user and those resources that correspond more closely to the user's intent. We validate this assertion through extensive evaluation of the proposed algorithm using data from a real collaborative tagging Web site. © 2008 Springer-Verlag Berlin Heidelberg.},
author_keywords={Collaborative tagging;  Hierarchical clustering;  Personalization},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Kim200842,
author={Kim, H. and Breslin, J.G. and Yang, S. and Song, S. and Kim, H.},
title={Int.ere.st: Building a tag sharing service with the SCOT ontology},
journal={AAAI Spring Symposium - Technical Report},
year={2008},
volume={SS-08-06},
pages={42-47},
note={cited By },
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-52649179381&partnerID=40&md5=6c6ffcb27c00b1bf596aa24bcf1d2131},
affiliation={Digital Enterprise Research Institute, National University of Ireland, Galway, IDA Business Park, Lower Dangan, Galway, Ireland; Biomedical Knowledge Engineering Lab., Seoul National University, 28-22 Yeonkun-Dong, Chongno-Ku, Seoul, South Korea},
abstract={Although a number of people have participated in tagging activity on many Web 2.0 sites and the sites allow users to get their tagging data through Mash-up APIs, there are many issues (e.g. tag semantics) associated with social tagging or folksonomies. Many studies have focused on semantics of a tag that offers a way of exact meaning of an individual tag or among the tags. Surprisingly, there has been minimal research regarding tag sharing or exchange. We suggest a semantic model, is called the 'SCOT (Semantic Social Cloud of Tags)', to represent the structure and the semantics of tagging data. We also discuss social tagging processes in order to provide semantic interoperability of tagging data among diverse sources. The int.ere.st web site aims to allow users to search and bookmark their social tagging data and to help the sharing and exchange of tagging data among people or heterogenous sources. We give an overview of int.ere.st with the social tagging processes and describe specific algorithms to support the processes. Copyright © 2008, Association for the Advancement of Artificial Intelligence (www.aaai.org). All rights reserved.},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Steels200898,
author={Steels, L. and Tisselli, E.},
title={Social tagging in community memories},
journal={AAAI Spring Symposium - Technical Report},
year={2008},
volume={SS-08-06},
pages={98-103},
note={cited By },
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-52449117999&partnerID=40&md5=5cb7d8b19bf3b298316c4f565d2545d2},
affiliation={AI Lab., Vrije Universiteit Brüssel, Sony Computer Science Lab., Paris, France; Sony Computer Science Lab., Paris, France},
abstract={Communities form in order to manage and defend a commons and community memories are distributed information systems that help them do so. This paper discusses the use of tagging within community memories. We argue that the role of tagging goes beyond being an aid in navigation. It is a means by which communities come to grips with the tensions and challenges of their environment through the bottom-up construction of a common ontology and a representation of themselves. Copyright © 2008, Association for the Advancement of Artificial Intelligence (www.aaai.org). All rights reserved.},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Chi200811,
author={Chi, E.H. and Pirolli, P. and Suh, B. and Kittur, A. and Pendleton, B. and Mytkowicz, T.},
title={Augmented Social Cognition},
journal={AAAI Spring Symposium - Technical Report},
year={2008},
volume={SS-08-06},
pages={11-17},
note={cited By },
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-52449111875&partnerID=40&md5=bc30b635b67d94bd60b24e5425e848f0},
affiliation={Palo Alto Research Center, 3333 Coyote Hill Road, Palo Alto, CA 94304, United States},
abstract={Research in Augmented Social Cognition is aimed at enhancing the ability of a group of people to remember, think, and reason; to augment their speed and capacity to acquire, produce, communicate, and use knowledge; and to advance collective and individual intelligence in socially mediated information environments. In this paper, we describe the emergence of this research endeavor, and summarize some results from the research. In particular, we have found that (1) analyses of conflicts and coordination in Wikipedia have shown us the scientific need to understand social sensemaking environments; and (2) information theoretic analyses of social tagging behavior in del.icio.us shows the need to understand human vocabulary systems.},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Stoyanovich2008104,
author={Stoyanovich, J. and Amer-Yahia, S. and Marlow, C. and Yu, C.},
title={Leveraging tagging to model user interests in del.icio.us},
journal={AAAI Spring Symposium - Technical Report},
year={2008},
volume={SS-08-06},
pages={104-109},
note={cited By },
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-52449112950&partnerID=40&md5=51699706da9674ef44dd18439c5f5c8a},
affiliation={Columbia University, New York, NY 10025, United States; Yahoo Research, New York, NY 10018, United States},
abstract={Social tagging sites such as Flickr, YouTube and del.icio.us are becoming increasingly popular. Users of these sites annotate and endorse content by tagging, and form social ties with other users by including them into their friendship network. The richness of social context raises the users' expectations with respect to the quality of served content, but also presents a unique opportunity for the design of semantically-enriched recommender systems. This paper presents a variety of methods for producing customized hotlists and evaluates their effectiveness on del.icio.us datasets. We model a user's interest in terms of the tags he uses to annotate content, and in terms of his explicitly stated and derived social ties, and demonstrate how such interest can be leveraged to produce holistic of very high quality. We also discuss possible research directions and outline strategies for the design of a social tagging recommender system. Copyright © 2008, Association for the Advancement of Artificial Intelligence (www.aaai.org). All rights reserved.},
document_type={Conference Paper},
source={Scopus},
}

