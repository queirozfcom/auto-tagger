title={Self-Taught Active Learning from crowds},
journal={Proceedings - IEEE International Conference on Data Mining, ICDM},
year={2012},
pages={858-863},
doi={10.1109/ICDM.2012.64},
art_number={6413841},
note={cited By },
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84874049835&doi=10.1109%2fICDM.2012.64&partnerID=40&md5=53eb96d89937b41474b736266917644d},
affiliation={Centre for Quantum Computation and Intelligent Sys., FEIT, Univ. of Technology, Sydney, NSW, Australia; Dept. of Computer and Electrical Eng. and Computer Science, Florida Atlantic University, FL, United States; Department of Computer Science, University of Massachusetts, Boston, MA, United States; Department of Computer Science, University of Vermont, Burlington, VT, United States; Department of Computer Science, Hefei University of Technology, Hefei, China},
abstract={The emergence of social tagging and crowdsourcing systems provides a unique platform where multiple weak labelers can form a crowd to fulfill a labeling task. Yet crowd labelers are often noisy, inaccurate, and have limited labeling knowledge, and worst of all, they act independently without seeking complementary knowledge from each other to improve labeling performance. In this paper, we propose a Self-Taught Active Learning (STAL) paradigm, where imperfect labelers are able to learn complementary knowledge from one another to expand their knowledge sets and benefit the underlying active learner. We employ a probabilistic model to characterize the knowledge of each labeler through which a weak labeler can learn complementary knowledge from a stronger peer. As a result, the self-taught active learning process eventually helps achieve high classification accuracy with minimized labeling costs and labeling errors. © 2012 IEEE.},
author_keywords={Active Learning;  Crowd;  Self-taught},
document_type={Conference Paper},
source={Scopus},
}

@ARTICLE{Li201264,
