url = "http://www.sciencedirect.com/science/article/pii/B9781843347224500047",
author = "Roberto Raieli",
keywords = "state of the art",
keywords = "user needs",
keywords = "digital reference",
keywords = "effectiveness",
keywords = "efficiency",
keywords = "human-machine interaction",
keywords = "computer understanding",
keywords = "semantic gap",
