  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-82955240658&doi=10.1115%2f1.3617447&partnerID=40&md5=b6b00a68af1567d88a895a06d6b272e1},
}

