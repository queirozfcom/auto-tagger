  source        = {Scopus},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-52149084150&doi=10.1109%2fICSC.2008.15&partnerID=40&md5=2a6c0527f4b4fb8b99e3bc90ea3430e1},
