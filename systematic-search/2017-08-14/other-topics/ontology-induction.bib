@Conference{djuana_etal_2012a,
  author          = {Djuana, E. and Xu, Y. and Li, Y. and Cox, C.},
  title           = {Personalization in tag ontology learning for recommendation making},
  year            = {2012},
  pages           = {368-377},
  note            = {cited By},
  abstract        = {Due to the explosive growth of the Web, the domain of Web personalization has gained great momentum both in the research and commercial areas. One of the most popular web personalization systems is recommender systems. In recommender systems choosing user information that can be used to profile users is very crucial for user profiling. In Web 2.0, one facility that can help users organize Web resources of their interest is user tagging systems. Exploring user tagging behavior provides a promising way for understanding users' information needs since tags are given directly by users. However, free and relatively uncontrolled vocabulary makes the user self-defined tags lack of standardization and semantic ambiguity. Also, the relationships among tags need to be explored since there are rich relationships among tags which could provide valuable information for us to better understand users. In this paper, we propose a novel approach for learning tag ontology based on the widely used lexical database WordNet for capturing the semantics and the structural relationships of tags. We present personalization strategies to disambiguate the semantics of tags by combining the opinion of WordNet lexicographers and users' tagging behavior together. To personalize further, clustering of users is performed to generate a more accurate ontology for a particular group of users. In order to evaluate the usefulness of the tag ontology, we use the tag ontology in a pilot tag recommendation experiment for improving the recommendation performance by exploiting the semantic information in the tag ontology. The initial result shows that the personalized information has improved the accuracy of the tag recommendation. ¬© 2012 ACM.},
  affiliation     = {School of Electrical Engineering and Computer Science, Queensland University of Technology, GPO Box 2434, Brisbane, QLD 4001, Australia; Rummble Labs., Rummble Ltd., 1 East Poultry Avenue, London, EC1A 9PT, United Kingdom},
  author_keywords = {clustering; collaborative tagging; ontology learning; recommendation; user interest; user profiling},
  document_type   = {Conference Paper},
  doi             = {10.1145/2428736.2428804},
  journal         = {ACM International Conference Proceeding Series},
  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84873338116&doi=10.1145%2f2428736.2428804&partnerID=40&md5=7b20d190dfda750433e2c8523e2d3ed7},
}

@Article{tsytsarau_etal_2012,
  author          = {Tsytsarau, M. and Bonchi, F. and Gionis, A. and Palpanas, T.},
  title           = {Diverse dimension decomposition for itemset spaces},
  journal         = {Knowledge and Information Systems},
  year            = {2012},
  volume          = {33},
  number          = {2},
  pages           = {447-473},
  note            = {cited By},
  abstract        = {We introduce the problem of diverse dimension decomposition in transactional databases, where a dimension is a set of mutually exclusive itemsets. The problem we consider requires to find a decomposition of the itemset space into dimensions, which are orthogonal to each other and which provide high coverage of the input database. The mining framework we propose can be interpreted as a dimensionality-reducing transformation from the space of all items to the space of orthogonal dimensions. Relying on information-theoretic concepts, we formulate the diverse dimension decomposition problem with a single objective function that simultaneously captures constraints on coverage, exclusivity, and orthogonality. We show that our problem is NP-hard, and we propose a greedy algorithm exploiting the well-known FP-tree data structure. Our algorithm is equipped with strategies for pruning the search space deriving directly from the objective function. We also prove a property that allows assessing the level of informativeness for newly added dimensions, thus allowing to define criteria for terminating the decomposition. We demonstrate the effectiveness of our solution by experimental evaluation on synthetic datasets with known dimension and three real-world datasets, flickr, del. icio. us and dblp. The problem we study is largely motivated by applications in the domain of collaborative tagging; however, the mining task we introduce in this paper is useful in other application domains as well. ¬© 2012 Springer-Verlag London Limited.},
  affiliation     = {via Sommarive 14, Povo, TN 38123, Italy; University of Trento, Trento, Italy; Yahoo Research, Barcelona, Spain},
  author_keywords = {Collaborative tagging; Dimension decomposition; Diversity; Information theory; Transactional databases},
  document_type   = {Article},
  doi             = {10.1007/s10115-012-0518-5},
  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84867694308&doi=10.1007%2fs10115-012-0518-5&partnerID=40&md5=a6008ffa0bb91fd3c0608f246e064894},
}

