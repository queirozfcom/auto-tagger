  source        = {Scopus},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-52249107128&doi=10.1109%2fIWSCA.2008.36&partnerID=40&md5=b84be3fc4b366661ae460759944fe898},
