@InProceedings{dallagnol_etal_2011,
  author    = {J. M. H. Dall'Agnol and C. A. Tacla and A. R. Freddo and A. H. Molinari and E. C. Para√≠o},
  title     = {The use of well-founded argumentation on the conceptual modeling of collaborative ontology development},
  booktitle = {Proceedings of the 2011 15th International Conference on Computer Supported Cooperative Work in Design (CSCWD)},
  year      = {2011},
  pages     = {200-207},
  month     = {June},
  abstract  = {Divergences in conceptual modeling choices are inherent to the collaborative ontology development. Such divergences have been typically solved through some process of negotiation among the participants of the development process. When negotiating, the participants argue to defend their ideas based on their past experiences. We propose to support and to guide the argumentation using philosophical notions brought by the OntoClean methodology to show the participants the consequences of their conceptual modeling choices. So the participants can choose the modeling option that better represents the intended model of the domain. This paper presents an approach for collaborative ontology development in a distributed way, specifically to support activities of the conceptualization phase and the whole process of discussion to reach consensus, aiming at developing ontologies that better fit the intended models for a domain.},
  doi       = {10.1109/CSCWD.2011.5960075},
  keywords  = {ontologies (artificial intelligence);OntoClean methodology;argumentation process;collaborative ontology development;conceptual modeling choice;negotiation process;Buildings;Collaboration;Nominations and elections;Ontologies;Tagging;Unified modeling language;Vocabulary;OntoClean;argumentation process;collaborative ontology development},
}

@InProceedings{ungrangsi_anutariya_2011,
  author    = {R. Ungrangsi and C. Anutariya},
  title     = {A comparison study of Flickr's folksonomies and ontologies},
  booktitle = {2011 Eighth International Joint Conference on Computer Science and Software Engineering (JCSSE)},
  year      = {2011},
  pages     = {257-262},
  month     = {May},
  abstract  = {Flickr is a widely-known Web 2.0 photo sharing system that allows users to describe their own photos with tags (aka. folksonomies) for indexing purposes. However, its tag-based photo retrieval function is severely hampered by the inherent nature of folksonomies. As the first step moving towards the alleviation of the problem, this paper, therefore, investigates characterizations of folksonomies on Flickr through its all-time most popular tags. In particular, the study focuses on tag clusters, related tags, and tag categorizations of the all-time most popular tags and compares them with concepts formally defined in ontologies from the Semantic Web. The experimental results imply the feasibilit of employing the Semantic Web technology in searching Flickr photos.},
  doi       = {10.1109/JCSSE.2011.5930131},
  keywords  = {classification;image retrieval;indexing;ontologies (artificial intelligence);semantic Web;social networking (online);Flickr;Web 2.0 photo sharing system;folksonomies;indexing;ontologies;semantic Web;tag categorizations;tag clusters;tag-based photo retrieval function;Semantic Web;Web2.0;folksonomy;ontology},
}

