  keywords      = {Web sites;data handling;groupware;ontologies (artificial intelligence);semantic Web;Web 2.0 sites;collaborative tagging spaces;data annotation;data reuse;data tagging;semantic Web;semantically-linked data;social tagging practice;tag data representation;tag ontologies;Blogs;Extraterrestrial phenomena;Indexing;Information retrieval;International collaboration;Licenses;Ontologies;Semantic Web;Tagging;Weaving;folksonomy;ontology;semantic web;tag ontology;tagging},
  source        = {Scopus},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-52149106642&doi=10.1109%2fICSC.2008.79&partnerID=40&md5=10435531eaa6a3301edd7d3d67c2b7f0},
