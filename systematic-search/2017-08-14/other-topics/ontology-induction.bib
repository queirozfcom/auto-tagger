  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-78449280708&doi=10.1109%2fDEXA.2010.53&partnerID=40&md5=e1ef20a787fcde12e972cd5819ee418d},
