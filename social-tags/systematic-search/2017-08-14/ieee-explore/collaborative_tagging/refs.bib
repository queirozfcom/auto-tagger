@INPROCEEDINGS{6644353,
author={X. Che and H. Yang and C. Meinel},
booktitle={2013 Second International Conference on E-Learning and E-Technologies in Education (ICEEE)},
title={Tree-structure outline generation for lecture videos with synchronized slides},
year={2013},
pages={87-92},
abstract={In this paper we propose a solution to generate tree-structure outline for lecture videos by analyzing their synchronized slides, by which detailed lecture overview can be automatically provided to E-learning portal users. Starting with OCR (Optical Character Recognition) result, we reconstruct the content of each slide. After that, we explore logical relations between slides, in order to make them hierarchical. And all potential redundant content will also be removed. Our evaluation shows that, based on our test dataset, the final outline achieved retains about 1/4 of the original texts from all slides and is organized well in an up-to-6-level tree structure. Furthermore, the average accuracy of all slide titles, which are undoubtedly the most important information, reaches 86%.},
