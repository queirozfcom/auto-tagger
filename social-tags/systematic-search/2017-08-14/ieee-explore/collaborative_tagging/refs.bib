@INPROCEEDINGS{6965088,
author={C. Deepa and A. S. Vijendran},
booktitle={2014 International Conference on Intelligent Computing Applications},
title={TAD-RR: Tag-Annotation Based Demand Re-ranking Approach for Personalized Image Retrieval},
