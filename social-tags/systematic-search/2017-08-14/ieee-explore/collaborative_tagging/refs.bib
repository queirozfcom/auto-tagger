@BOOK{6976707,
author={Marjorie Hlava},
booktitle={The Taxobook: Principles and Practices of Taxonomy Construction (Part 2 of a 3-Part Series):Principles and Practices of Building Taxonomies, Part 2 of a 3-Part Series},
