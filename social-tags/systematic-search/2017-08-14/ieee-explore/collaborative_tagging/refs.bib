@ARTICLE{6742613,
author={M. Katsurai and T. Ogawa and M. Haseyama},
journal={IEEE Transactions on Multimedia},
title={A Cross-Modal Approach for Extracting Semantic Relationships Between Concepts Using Tagged Images},
year={2014},
